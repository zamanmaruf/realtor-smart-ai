{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/admin",
      "dest": "server/index.js"
    },
    {
      "src": "/real-estate-bg.jpeg",
      "dest": "/public/real-estate-bg.jpeg"
    },
    {
      "src": "/property1.jpg",
      "dest": "/public/property1.jpg"
    },
    {
      "src": "/property2.jpg",
      "dest": "/public/property2.jpg"
    },
    {
      "src": "/property3.jpg",
      "dest": "/public/property3.jpg"
    },
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/chatbot.js",
      "dest": "/public/chatbot.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/public/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/public/sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 